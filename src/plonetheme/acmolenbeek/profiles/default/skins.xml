<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="plonetheme.acmolenbeek">

 <object name="acmolenbeek_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.acmolenbeek:skins/acmolenbeek_images"/>
 <object name="acmolenbeek_templates"
    meta_type="Filesystem Directory View"
    directory="plonetheme.acmolenbeek:skins/acmolenbeek_templates"/>
 <object name="acmolenbeek_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.acmolenbeek:skins/acmolenbeek_styles"/>

 <skin-path name="plonetheme.acmolenbeek" based-on="Plone Classic Theme">
  <layer name="acmolenbeek_images"
     insert-after="custom"/>
  <layer name="acmolenbeek_templates"
     insert-after="acmolenbeek_images"/>
  <layer name="acmolenbeek_styles"
     insert-after="acmolenbeek_templates"/>
 </skin-path>

</object>
